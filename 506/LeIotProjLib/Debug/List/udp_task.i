    UINT8 equipment_ip[16]={0};
    UINT8 adapter_ver[8]={0};
    SDKInformation broadcast_data;
    socklen_t addrlen = sizeof(struct sockaddr_in);
    _udp_init(&sockfd,&prot_flag,&servaddr);
    rtl_sprintf(adapter_ver,"%d",1000);

    while(1)
    {
        _strcpy(equipment_ip, IOTWifi_LocalIP());
        if(IOTSDK_GetInformation(&broadcast_data)<0)
        {
            do{if(g_log_level >= 2) {rtl_printf("[D]%s"  "broadcast_data error\n",rtos_time_print());}}while(0);
        }else{
            json_create(&send_buf,equipment_ip,adapter_ver,&broadcast_data);
        }
        
        if(send_message_flag)
        {
            if(lwip_sendto(sockfd,send_buf,prvStrLen((const unsigned char *) send_buf),0,(struct sockaddr *) &servaddr,addrlen)<0)
                do{if(g_log_level >= 0) {rtl_printf("[D]%s"  "send error!errno=%d\n",rtos_time_print(),errno);}}while(0);
            do{if(g_log_level >= 0) {rtl_printf("[D]%s"  "client: IPAddr = %s, Port = %d, send_buf = %s\n",rtos_time_print(),ipaddr_ntoa((ip_addr_t*)&(servaddr . sin_addr)), lwip_ntohs(servaddr . sin_port),send_buf);}}while(0);
            vPortFree(send_buf);
        }
        vTaskDelay(2000/( ( TickType_t ) 1000 / ( ( uint32_t ) 1000 ) ));
    }
    
}
void udp_send_task(void *arg)
{
    _udp_send();
}

